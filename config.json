{
    "batch_size": 10,
    "epochs": 100,
    "model": "GSCEventMOD",
    "pipeline_type": "step-prediction",
    "model_parameters": {
        "restore_model": false,
        "num_grouped_events": 10,
        "parameters": {
            "min_num_clusters": 2,
            "max_num_clusters": 6,
            "num_neighbors": 30,
            "num_clusters": 2,
            "mode": "find-optimal-clusters"
        }
    },
    "aedat_file_reader_config": {
        "path": "./Cars_sequence.aedat4"
    },
    "visualize": true,
    "data_processors": {
        "class_balanced_split": "",
        "data_input_path": "/Users/krishnasharma/Desktop/model_data",
        "train_validation_test_split": {
            "train_percentage": 60,
            "validation_percentage": 30,
            "test_percentage": 10
        },
        "steps": [{
                "name": "crop",
                "parameters": {
                    "x_left": 0,
                    "x_right": 10,
                    "y_top": 100,
                    "y_bottom": 200
                }
            },
            {
                "name": "noise_filter",
                "parameters": {
                    "load_model": false,
                    "model_path": "./",
                    "random_state": 10,
                    "n_jobs": -1,
                    "contamination": 10
                }
            }
        ]
    },
    "data_transformers": [{
        "name": "normalize",
        "parameters": {
            "output_range_min": 0,
            "output_range_max": 10
        }
    }]
}